{
  "name": "phisher-man-react",
  "version": "1.0.0",
  "description": "Modern React/Node.js version of Phisher-Man with improved UI",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-client": "cd client && npm install",
    "heroku-postbuild": "npm run install-client && npm run build"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "morgan": "^1.10.0",
    "fs-extra": "^11.1.1",
    "path": "^0.12.7",
    "child_process": "^1.0.2",
    "socket.io": "^4.7.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.0"
  },
  "keywords": [
    "phishing",
    "security",
    "testing",
    "react",
    "nodejs"
  ],
  "author": "FDX100",
  "license": "MIT"
}
